<%= form_for @product, url: {action: "create"} do |f| %>
  <%= f.label :Name %>
  <%= f.text_field :name %><br>
  <%= f.label :Price %>
  <%= f.text_field :price %><br>
  <%= f.label :Components %>
  <%= f.text_field :components %><br>
  <%= f.label :Available_count %>
  <%= f.text_field :available_count %><br>
  <%= f.submit "Create" %>
<% end %>